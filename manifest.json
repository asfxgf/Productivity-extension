// manifest.json
{
  "name": "The efficient extension",
  "version": "1.0",
  "description": "Affiche un compteur affichant son age actuel sur google.com",
  "manifest_version": 2,
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://www.google.fr/", "https://www.google.com/"]
  }],
  "permissions": [
    "activeTab",
    "https://www.example.com/*"
  ]
}
